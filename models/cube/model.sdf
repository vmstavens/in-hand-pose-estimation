<!-- cube as a test object for in hand pose estimation -->
<sdf version="1.4">
	<model name="cube">
		<pose>0 0 0 0 0 0</pose>
		<static>true</static>
		<link name="link">
			<collision name="collision">
				<geometry>
					<box>
						<size>0.1 0.1 0.1</size>
					</box>
				</geometry>
			</collision>
			<visual name="visual">
				<geometry>
					<box>
						<size>0.1 0.1 0.1</size>
					</box>
				</geometry>
			</visual>
		</link>
	</model>
</sdf>