	<model name="edge">
		<static>true</static>
		<link name="base_link">
			<collision name="collision">
				<geometry>
					<box>
						<size>0.05 0.05 0.1</size>
					</box>
				</geometry>
			</collision>
			<visual name="visual">
				<geometry>
					<box>
						<size>0.05 0.05 0.1</size>
					</box>
				</geometry>
			</visual>
		</link>
	</model>
</sdf>